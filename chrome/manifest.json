{
  "manifest_version": 2,
  "name": "Audio API Tracer",
  "description": "Keeps track of the audio graph.",
  "version": "0.1.0",
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at" : "document_start",
      "js": ["js/tab-page-changed.js"],
      "all_frames": false
    },
    {
      "matches": ["<all_urls>"],
      "run_at" : "document_start",
      "js": ["js/inject-tracing.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "images/*",
    "js/*"
  ],
  "icons": {"48": "media/images/icon.png"},
  "devtools_page": "dev-tools.html"
}
